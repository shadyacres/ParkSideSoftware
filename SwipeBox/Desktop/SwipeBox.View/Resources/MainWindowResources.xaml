<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SwipeBox.Presentation;assembly=SwipeBox.Presentation"
                    xmlns:vw="clr-namespace:SwipeBox.View">


    <!-- Menu-->
    <DataTemplate DataType="{x:Type vm:MenuViewModel}">
        <vw:MenuView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <vw:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ClientsViewModel}">
        <vw:ClientsView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <vw:SettingsView />
    </DataTemplate>
</ResourceDictionary>